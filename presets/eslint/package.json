{
  "name": "@mscharley/eslint-config",
  "version": "1.3.3",
  "description": "ESLint presets that I use a lot",
  "main": "index.json",
  "scripts": {
    "build": "",
    "precommit": "lint-staged",
    "test": "eslint -c index.js *.js"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{json}": "prettier -w"
