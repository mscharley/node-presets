{
  "name": "@mscharley/bootstrap-node",
  "version": "0.0.16",
  "description": "Bootstrap an opinionated node project quickly.",
  "bin": {
    "bootstrap": "dist/bin/bootstrap.js"
  },
  "scripts": {
    "build": "tsc",
    "precommit": "lint-staged",
    "check-updates": "npm-check-updates 2>&1",
    "test": "rushx test:jest",
    "test:lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "test:jest": "jest 2>&1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.json": "prettier -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mscharley/node-presets.git"
  },
  "keywords": [
    "node"
  ],
  "author": "Matthew Scharley <matt@scharley.me>",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/mscharley"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mscharley/node-presets/issues"
  },
  "homepage": "https://github.com/mscharley/node-presets#readme",
  "devDependencies": {
    "@babel/core": "~7.17.5",
    "@babel/preset-env": "~7.16.11",
    "@babel/preset-typescript": "~7.16.7",
    "@typescript-eslint/parser": "^5.13.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@mscharley/eslint-config": "workspace:*",
    "@mscharley/prettier-config": "workspace:*",
    "@types/jest": "~27.4.1",
    "@types/node": "~17.0.21",
    "babel-jest": "~27.5.1",
    "bufferutil": "~4.0.6",
    "canvas": "~2.9.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "~12.3.4",
    "jest": "~27.5.1",
    "prettier": "^2.5.1",
    "typescript": "~4.6.2",
    "utf-8-validate": "~5.0.8",
    "npm-check-updates": "~12.5.0"
  }
}
