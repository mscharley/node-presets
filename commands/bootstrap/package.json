{
  "name": "@mscharley/bootstrap-node",
  "version": "1.0.0",
  "description": "Bootstrap an opinionated node project quickly.",
  "bin": {
    "bootstrap": "dist/bin/bootstrap.js"
  },
  "scripts": {
    "build": "tsc",
    "precommit": "lint-staged",
    "check-updates": "npm-check-updates 2>&1",
    "test": "rushx test:jest",
    "test:lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "test:jest": "jest 2>&1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.json": "prettier -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mscharley/node-presets.git"
  },
  "keywords": [
    "node"
  ],
  "author": "Matthew Scharley <matt@scharley.me>",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/mscharley"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mscharley/node-presets/issues"
  },
  "homepage": "https://github.com/mscharley/node-presets#readme",
  "devDependencies": {
    "@babel/core": "~7.21.3",
    "@babel/preset-env": "~7.20.2",
    "@babel/preset-typescript": "~7.21.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@mscharley/eslint-config": "workspace:*",
    "@mscharley/prettier-config": "workspace:*",
    "@types/jest": "~29.5.0",
    "@types/node": "~18.15.3",
    "babel-jest": "~29.5.0",
    "bufferutil": "~4.0.7",
    "canvas": "~2.11.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-deprecation": "^1.4.0-next.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "~13.2.0",
    "jest": "~29.5.0",
    "prettier": "^2.8.5",
    "typescript": "~5.0.2",
    "utf-8-validate": "~6.0.3",
    "npm-check-updates": "~16.7.13"
  }
}
