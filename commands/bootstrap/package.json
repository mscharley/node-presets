{
	"name": "@mscharley/bootstrap-node",
	"version": "1.0.4",
	"publishConfig": {
		"provenance": true
	},
	"private": true,
	"description": "Bootstrap an opinionated node project quickly.",
	"bin": {
		"bootstrap": "dist/bin/bootstrap.js"
	},
	"scripts": {
		"build": "tsc",
		"precommit": "lint-staged",
		"check-updates": "npm-check-updates 2>&1",
		"test": "npm run test:jest",
		"test:lint": "eslint --ext .js,.jsx,.ts,.tsx .",
		"test:jest": "jest 2>&1"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "eslint --fix",
		"*.json": "prettier -w"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mscharley/node-presets.git"
	},
	"keywords": [
		"node"
	],
	"author": "Matthew Scharley <matt@scharley.me>",
	"funding": {
		"type": "individual",
		"url": "https://github.com/sponsors/mscharley"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/mscharley/node-presets/issues"
	},
	"homepage": "https://github.com/mscharley/node-presets#readme",
	"devDependencies": {
		"@babel/core": "7.22.20",
		"@babel/preset-env": "7.22.20",
		"@babel/preset-typescript": "7.22.15",
		"@typescript-eslint/parser": "5.62.0",
		"@typescript-eslint/eslint-plugin": "5.62.0",
		"@mscharley/eslint-config": "2.0.5",
		"@mscharley/prettier-config": "2.0.3",
		"@types/jest": "29.5.6",
		"@types/node": "20.3.3",
		"babel-jest": "29.5.0",
		"bufferutil": "4.0.8",
		"canvas": "2.11.2",
		"eslint": "8.52.0",
		"eslint-config-prettier": "8.10.0",
		"eslint-plugin-deprecation": "1.5.0",
		"eslint-plugin-node": "11.1.0",
		"eslint-plugin-prettier": "4.2.1",
		"lint-staged": "13.2.3",
		"jest": "29.5.0",
		"prettier": "2.8.8",
		"typescript": "5.1.6",
		"utf-8-validate": "6.0.3",
		"npm-check-updates": "16.14.6"
	}
}
