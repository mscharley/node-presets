{
  "name": "@mscharley/bootstrap-node",
  "version": "0.0.18",
  "description": "Bootstrap an opinionated node project quickly.",
  "bin": {
    "bootstrap": "dist/bin/bootstrap.js"
  },
  "scripts": {
    "build": "tsc",
    "precommit": "lint-staged",
    "check-updates": "npm-check-updates 2>&1",
    "test": "rushx test:jest",
    "test:lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "test:jest": "jest 2>&1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.json": "prettier -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mscharley/node-presets.git"
  },
  "keywords": [
    "node"
  ],
  "author": "Matthew Scharley <matt@scharley.me>",
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/mscharley"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mscharley/node-presets/issues"
  },
  "homepage": "https://github.com/mscharley/node-presets#readme",
  "devDependencies": {
    "@babel/core": "~7.19.1",
    "@babel/preset-env": "~7.19.1",
    "@babel/preset-typescript": "~7.18.6",
    "@typescript-eslint/parser": "^5.37.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@mscharley/eslint-config": "workspace:*",
    "@mscharley/prettier-config": "workspace:*",
    "@types/jest": "~29.0.2",
    "@types/node": "~18.7.18",
    "babel-jest": "~29.0.3",
    "bufferutil": "~4.0.6",
    "canvas": "~2.10.1",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "~13.0.3",
    "jest": "~29.0.3",
    "prettier": "^2.7.1",
    "typescript": "~4.8.3",
    "utf-8-validate": "~5.0.9",
    "npm-check-updates": "~16.1.2"
  }
}
